msgid "\t--noreject\t\t-N\tDon't reject viruses, silently throw them away."
msgstr ""

#: clamav-milter.c:969
msgid "\t--noxheader\t\t-n\tSuppress X-Virus-Scanned/X-Virus-Status headers."
msgstr ""

